<%= simple_form_for(@celebrity) do |f| %>
    <%= f.input :name %>
    <%= f.input :category, collection: ["film", "music", "sports", "politics", "influencer" ] %>
    <%= f.input :description %>
    <%= f.input :daily_rate %>
    <%# <%= f.input :photo, as: :file %>
    <%= f.submit %>
  <% end %>

<%= link_to "All Celebs", celebrities_path %>
